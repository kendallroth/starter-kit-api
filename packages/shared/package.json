{
  "name": "@starter-kit-api/shared",
  "version": "1.0.0",
  "description": "Shared utilities for `@starter-kit-api`",
  "author": "Kendall Roth <kendall@kendallroth.ca>",
  "license": "MIT",
  "files": ["build"],
  "main": "./build/index.js",
  "module": "./build/index.mjs",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "tsup ./src/index.ts --format cjs,esm --dts --clean --out-dir build",
    "dev": "npm run build -- --watch",
    "lint": "npm run lint:code && npm run lint:types",
    "lint:code": "biome check src/ --apply",
    "lint:types": "tsc"
  }
}
