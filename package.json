{
  "name": "@kendallroth/starter-kit-api",
  "version": "1.0.0",
  "description": "Simple API for `starter-kit-*` projects",
  "exports": {
    "server": {
      "import": "./build/server.mjs",
      "require": "./build/server.js"
    },
    "shared": {
      "import": "./build/shared.mjs",
      "require": "./build/shared.js"
    }
  },
  "scripts": {
    "prebuild": "npm run lint:types",
    "build": "npm run build:bundle && npm run build:generate",
    "build:bundle": "tsup",
    "build:generate": "tsoa spec-and-routes --configuration src/server/tsoa.json && copyfiles src/server/generated/swagger.json build --up 3",
    "dev": "npm run build -- --watch",
    "lint": "npm run lint:code && npm run lint:types",
    "lint:code": "biome check src/ --apply",
    "lint:types": "tsc"
  },
  "license": "MIT",
  "dependencies": {
    "@biomejs/biome": "^1.4.1",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "fast-sort": "^3.4.0",
    "jsonwebtoken": "^9.0.2",
    "lowdb": "github:dankreiger/lowdb#chore/esm-cjs-hybrid-WITH-LIB",
    "swagger-ui-express": "^5.0.0",
    "ts-deepmerge": "^6.2.0",
    "tsoa": "^5.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "copyfiles": "^2.4.1"
  }
}
